Package: phuse
Type: Package
Title: Web Application Framework for Phuse Scripts
Version: 0.1.2
Author: Hanming Tu [aut, cre]
Authors@R: c(
  person("Hanming", "Tu", email = "hanming.tu@gmail.com",role = c("aut", "cre"))
